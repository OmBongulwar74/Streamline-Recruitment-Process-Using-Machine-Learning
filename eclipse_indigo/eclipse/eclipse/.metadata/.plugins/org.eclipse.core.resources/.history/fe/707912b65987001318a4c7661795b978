drop table  Persons1;
CREATE TABLE Persons1
(
emailid varchar(25) NOT NULL,
password varchar(15),
usertype varchar(15),
PRIMARY KEY (emailid)
)

CREATE TABLE Login
(
emailid varchar(25),
password varchar(15),
usertype varchar(15),
PRIMARY KEY (emailid)
)

CREATE TABLE Registration
(

firstname varchar(15),
lastname varchar(15),
dob varchar(10),
Address varchar(50),
Country varchar(15),
emailid varchar(15) NOT NULL,
FOREIGN KEY (emailid) REFERENCES Login(emailid)
)